                                    android:layout_width="wrap_content"

                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="@dimen/tvMessageActivityMargin"
                                    android:layout_marginRight="@dimen/tvMessageActivityMargin"
                                    android:layout_weight="1"

                                    android:gravity="right|center"
                                    android:text="افسردگی و بیمار قلبی و عروقی   "
                                    android:textColor="@color/unselected_cardview_text"
                                    android:textSize="@dimen/list_title_text" />
                            </TableRow>


                        </LinearLayout>


                    </LinearLayout>

                </android.support.v7.widget.CardView>

                <com.github.aakira.expandablelayout.ExpandableRelativeLayout
                    android:id="@+id/what_is_ezterab_expandableLayout2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginEnd="5sp"
                    android:layout_marginStart="5sp"
                    app:ael_duration="500"
                    app:ael_expanded="false"
                    app:ael_interpolator="bounce"
                    app:ael_orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <android.support.v7.widget.CardView
                            android:id="@+id/what_is_ezterab_cardview2_2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:clickable="true"
                            android:foreground="?android:attr/selectableItemBackground">

                            >

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="vertical">


                                <TableRow
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1">

                                    <com.codesgood.views.JustifiedTextView
                                        android:id="@+id/what_is_ezterab_textView2"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"

                                        android:justificationMode="inter_word"
                                        android:padding="10sp"
                                        android:textAlignment="textStart"
                                        android:textSize="@dimen/normal_text"></com.codesgood.views.JustifiedTextView>

                                </TableRow>

                            </LinearLayout>

                        </android.support.v7.widget.CardView>

                    </LinearLayout>

                </com.github.aakira.expandablelayout.ExpandableRelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="8dp"
                android:orientation="vertical">

                <android.support.v7.widget.CardView
                    android:id="@+id/what_is_ezterab_cardview3"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/expandible_cv"
                    android:clickable="true"
                    app:cardBackgroundColor="@color/unselect_cardview">

                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">


                        <com.github.zagum.expandicon.ExpandIconView
                            android:id="@+id/eiv3"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="9dp"
                            android:layout_weight="3"
                            app:eiv_colorLess="@color/unselected_cardview_text"
                            app:eiv_colorMore="@color/selected_cardview_text"
                            app:eiv_switchColor="true" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <TableRow
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <TextView
                                    android:id="@+id/what_is_ezterab_title3"
                                    android:layout_width="wrap_content"

                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="@dimen/tvMessageActivityMargin"
                                    android:layout_marginRight="@dimen/tvMessageActivityMargin"
                                    android:layout_weight="1"

                                    android:gravity="right|center"
                                    android:text="علائم و نشانه های افسردگی  "
                                    android:textColor="@color/unselected_cardview_text"
                                    android:textSize="@dimen/list_title_text" />
                            </TableRow>


                        </LinearLayout>


                    </LinearLayout>

                </android.support.v7.widget.CardView>

                <com.github.aakira.expandablelayout.ExpandableRelativeLayout
                    android:id="@+id/what_is_ezterab_expandableLayout3"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginEnd="5sp"
                    android:layout_marginStart="5sp"
                    app:ael_duration="500"
                    app:ael_expanded="false"
                    app:ael_interpolator="bounce"
                    app:ael_orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <android.support.v7.widget.CardView
                            android:id="@+id/what_is_ezterab_cardview3_2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:clickable="true"
                            android:foreground="?android:attr/selectableItemBackground">

                            >

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="vertical">


                                <TableRow
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1">

                                    <com.codesgood.views.JustifiedTextView
                                        android:id="@+id/what_is_ezterab_textView3"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:justificationMode="inter_word"
