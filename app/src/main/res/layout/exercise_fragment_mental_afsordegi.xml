<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:card_view="http://schemas.android.com/tools"
    android:background="@color/white"

    android:layoutDirection="ltr"
    android:orientation="vertical">


    <ScrollView
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="8dp"
                android:orientation="vertical">

                <android.support.v7.widget.CardView
                    android:id="@+id/what_is_ezterab_cardview1"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/expandible_cv"
                    card_view:cardUseCompatPadding="false"
                    card_view:cardPreventCornerOverlap="false"
                    card_view:elevation="0dp"

                    app:cardBackgroundColor="@color/unselect_cardview">

                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">


                        <com.github.zagum.expandicon.ExpandIconView
                            android:id="@+id/eiv1"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="9dp"
                            android:layout_weight="3"
                            app:eiv_colorLess="@color/unselected_cardview_text"
                            app:eiv_colorMore="@color/selected_cardview_text"
                            app:eiv_switchColor="true" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <TableRow
                                android:layout_width="match_parent"
                                android:layout_height="match_parent">

                                <TextView
                                    android:id="@+id/what_is_ezterab_title1"
                                    android:layout_width="wrap_content"

                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="@dimen/tvMessageActivityMargin"
                                    android:layout_marginRight="@dimen/tvMessageActivityMargin"
                                    android:layout_weight="1"

                                    android:gravity="right|center"
                                    android:text="افسردگی چیست؟  "
                                    android:textColor="@color/unselected_cardview_text"
                                    android:textSize="@dimen/list_title_text" />
                            </TableRow>


                        </LinearLayout>


                    </LinearLayout>

                </android.support.v7.widget.CardView>

                <com.github.aakira.expandablelayout.ExpandableRelativeLayout
                    android:id="@+id/what_is_ezterab_expandableLayout1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"

                    android:layout_marginEnd="5dp"
                    android:layout_marginStart="5dp"
                    android:elevation="0dp"
                    app:ael_duration="500"
                    app:ael_expanded="false"
                    app:ael_interpolator="bounce"
                    app:ael_orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <android.support.v7.widget.CardView
                            android:id="@+id/what_is_ezterab_cardview1_2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:clickable="true"
                            android:foreground="?android:attr/selectableItemBackground">

                            >

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="vertical">


                                <TableRow
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1">

                                    <com.codesgood.views.JustifiedTextView
                                        android:id="@+id/what_is_ezterab_textView1"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:justificationMode="inter_word"
                                        android:padding="10sp"
                                        android:textAlignment="textStart"
                                        android:textSize="@dimen/normal_text"></com.codesgood.views.JustifiedTextView>

                                </TableRow>

                            </LinearLayout>

                        </android.support.v7.widget.CardView>

                    </LinearLayout>

                </com.github.aakira.expandablelayout.ExpandableRelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="8dp"
                android:orientation="vertical">

                <android.support.v7.widget.CardView
                    android:id="@+id/what_is_ezterab_cardview2"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/expandible_cv"
                    android:clickable="true"
                    app:cardBackgroundColor="@color/unselect_cardview">

                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">


                        <com.github.zagum.expandicon.ExpandIconView
                            android:id="@+id/eiv2"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_margin="9dp"
                            android:layout_weight="3"
                            app:eiv_colorLess="@color/unselected_cardview_text"
                            app:eiv_colorMore="@color/selected_cardview_text"
